    <profiles>
        <profile>
            <id>war</id>
            <build>
                <plugins>
                    <plugin>
                        <groupId>pt.ist</groupId>
                        <artifactId>ff-maven-plugin</artifactId>
                        <version>${version.pt.ist.ff.maven.plugin}</version>
                        <configuration>
                            <codeGeneratorClassName>pt.ist.bennu.backend.codeGenerator.VirtualHostAwareCodeGenerator</codeGeneratorClassName>
                        </configuration>
                        <dependencies>
                            <dependency>
                                <groupId>pt.ist</groupId>
                                <artifactId>fenix-framework-backend-jvstm-ojb-code-generator</artifactId>
                                <version>${version.fenix.framework}</version>
                            </dependency>
                            <dependency>
                                <groupId>pt.ist</groupId>
                                <artifactId>bennu-backend-util</artifactId>
                                <version>${version.bennu.backend.util}</version>
                            </dependency>
                        </dependencies>
                        <executions>
                            <execution>
                                <id>default</id>
                                <goals>
                                    <goal>ff-generate-domain</goal>
                                    <goal>ff-post-compile</goal>
                                    <goal>ff-process-atomic-annotations</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-war-plugin</artifactId>
                        <version>${version.org.apache.maven.plugins.maven.war.plugin}</version>
                        <configuration>
                            <packagingExcludes>**/web-fragment.xml</packagingExcludes>
                        </configuration>
                        <executions>
                            <execution>
                                <id>package</id>
                                <phase>package</phase>
                                <goals>
                                    <goal>war</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>pt.ist</groupId>
                        <artifactId>bennu-maven-plugin</artifactId>
                        <version>${version.pt.ist.bennu.maven.plugin}</version>
                        <configuration>
                            <webFragment>src/main/webapp/WEB-INF/web.xml</webFragment>
                        </configuration>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>update-web-fragment</goal>
                                    <goal>post-compile</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
